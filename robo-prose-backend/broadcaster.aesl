
<!DOCTYPE aesl-source>
<network>


<!--list of global events-->
<event size="0" id="0" name="tapped"/>

<!--list of constants-->


<!--show keywords state-->
<keywords flag="true"/>


<!--node thymio-II-->
<node name="thymio-II">

onevent buttons
if button.backward == 1 or button.center == 1 or button.forward == 1 or button.left == 1 or button.right == 1 then
    emit tapped
end


onevent button.forward
motor.left.target = 0
motor.right.target = 0

</node>


</network>
