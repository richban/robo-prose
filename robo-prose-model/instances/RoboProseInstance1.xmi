<?xml version="1.0" encoding="UTF-8"?>
<roboprose:RoboProse
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:roboprose="http://www.itu.dk/dsl/roboprose"
    xsi:schemaLocation="http://www.itu.dk/dsl/roboprose RoboProse.ecore">
  <main>
    <actions
        xsi:type="roboprose:Move"
        duration="10.0"/>
  </main>
  <listeners>
    <event xsi:type="roboprose:Obstacle"/>
    <actions
        xsi:type="roboprose:Turn"/>
    <actions
        xsi:type="roboprose:Move"
        duration="20.0"/>
  </listeners>
  <listeners>
    <event xsi:type="roboprose:Tapped"/>
    <actions
        xsi:type="roboprose:Stop"/>
  </listeners>
</roboprose:RoboProse>
